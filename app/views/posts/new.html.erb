<h1>New Post</h1>

<%= form_with :model => @post do |form| %>
  <%# add form fields %>
 <p>
    <%= form.label "author" %>
    <%= form.text_field "author" %>
    
    <%= form.label "image", "image URL" %>
    <%= form.text_field "image" %>
    
    <%= form.label "body" %>
    <%= form.text_field "body" %>
  </p>



  <%= form.submit %>
<% end %>
